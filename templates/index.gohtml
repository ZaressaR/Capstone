<div class="container">
<div class="center">
  <div class="left">
    <div class="header">
      <h2 class="animation a1">RX ROUTINE APP</h2>
      <h4 class="animation a2">NEVER MISS ANOTHER DOSE</h4>
    </div>

<form method="POST" action="/patient">
 <label for="firstName">Enter: </label>
  <input type="text" class="form-field animation a3" name="firstName" placeholder="First name"><br>
    
    <label for="lastName">Enter: </label>
 <input type="text" class="form-field animation a4" name="lastName" placeholder="Last name"><br>

<label for="rxname">Name of: </label>
  <input type="text" class="form-field animation a5" id="rxname" name="rxname" placeholder="Medication">
  <br>
  
  <label for="rxschedule">Scheduled for: </label>
  <input type="text" class="form-field animation a6" name="rxschedule" placeholder="AM or PM">
  <br>
  <button class="animation a7">Submit</button>
   </div>
  </div>
  <div class="right"></div>
</div>
</form>

<head>
<style>
* { box-sizing: border-box; }
@import url('https://fonts.googleapis.com/css?family=Rubik:400,500&display=swap');


body {
.center {
  font-family: 'Rubik', sans-serif;
  
  min-width:250px;
  max-width: 500px;
  margin: 20px;
  border: 15px rgba(0,0,0,.1) solid;
  padding: 50px;
  background-color: #f5f5f5;
  background-image: url(data:image/jpeg;base64,);

  
 }

}



.container {
  display: flex;
  height: 100vh;
 
}

.left {
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: center;
  animation-name: left;
  animation-duration: 1s;
  animation-fill-mode: both;
  animation-delay: 1s;
}

.right {
  flex: 1;
  background-color: black;
  transition: 1s;
  background-image: url(https://i.pinimg.com/originals/21/db/af/21dbafaa997a625e4dcb3804326b2011.jpg);
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}



.header > h2 {
  margin: 0;
  color: #4f46a5;
}

.header > h4 {
  margin-top: 10px;
  font-weight: normal;
  font-size: 20px;
  color: rgba(0,0,0,.4);
}

.form {
  max-width: 80%;
  display: flex;
  flex-direction: column;
}

.form > p {
  text-align: right;
}

.form > p > a {
  color: #000;
  font-size: 14px;
}

.form-field {
  height: 46px;
  padding: 0 16px;
  border: 2px solid #ddd;
  border-radius: 4px;
  font-family: 'Rubik', sans-serif;
  outline: 0;
  transition: .2s;
  margin-top: 20px;
}

.form-field:focus {
  border-color: #0f7ef1;
}

.form > button {
  padding: 12px 10px;
  border: 0;
  background: linear-gradient(to right, #de48b5 0%,#0097ff 100%); 
  border-radius: 3px;
  margin-top: 10px;
  color: rgba(0,0,0,.4)
  letter-spacing: 4px;
  font-family: 'Rubik', sans-serif;
}

.animation {
  animation-name: move;
  animation-duration: .4s;
  animation-fill-mode: both;
  animation-delay: 2s;
}

.a1 {
  animation-delay: 2s;
}

.a2 {
  animation-delay: 2.1s;
}

.a3 {
  animation-delay: 2.2s;
}

.a4 {
  animation-delay: 2.3s;
}

.a5 {
  animation-delay: 2.4s;
}

.a6 {
  animation-delay: 2.5s;
}
.a7
  animation-delay: 2.5s;
}
.a8 {
  animation-delay: 2.6s;
}

@keyframes move {
  0% {
    opacity: 0;
    visibility: hidden;
    transform: translateY(-40px);
  }

  100% {
    opacity: 1;
    visibility: visible;
    transform: translateY(0);
  }
}

@keyframes left {
  0% {
    opacity: 0;
    width: 0;
  }
   100% {
    opacity: 1;
    padding: 20px 40px;
    width: 440px;
  }
}
</style>
</head>
<body>
